DOXYFILE_ENCODING = UTF-8
PROJECT_NAME = @CMAKE_PROJECT_NAME@
PROJECT_NUMBER = @CMAKE_PROJECT_VERSION@
PROJECT_BRIEF = @CMAKE_PROJECT_DESCRIPTION@
OUTPUT_DIRECTORY = @CMAKE_SOURCE_DIR@/docs/source/
CREATE_SUBDIRS = NO
OUTPUT_LANGUAGE = English
ALWAYS_DETAILED_SEC = NO
OPTIMIZE_OUTPUT_FOR_C = YES
EXTRACT_PRIVATE = NO
EXTRACT_STATIC = NO
HIDE_UNDOC_MEMBERS = YES
HIDE_UNDOC_CLASSES = YES
FILE_PATTERNS = *.cpp *.h *.hpp
USE_MATHJAX = YES
FORMULA_TRANSPARENT = YES
FORMULA_FONTSIZE = 10
GENERATE_HTML = YES
GENERATE_LATEX = NO
GENERATE_XML = YES
XML_OUTPUT = doxydoc
# COMPACT_XML = YES
# RTF_HYPERLINKS = NO
BUILTIN_STL_SUPPORT = YES
EXTRACT_ALL = YES
INPUT = @CMAKE_CURRENT_SOURCE_DIR@/../include/
RECURSIVE = NO
HIDE_UNDOC_RELATIONS = YES
ENABLE_PREPROCESSING = YES
PREDEFINED = "_OPENMP __view__"

TAGFILES = @CMAKE_CURRENT_SOURCE_DIR@/doxygen-tags/opencv.tag=http://docs.opencv.org/4.5.2 \
           @CMAKE_CURRENT_SOURCE_DIR@/doxygen-tags/eigen.tag=https://eigen.tuxfamily.org/dox
GENERATE_TAGFILE = @CMAKE_CURRENT_SOURCE_DIR@/doxygen-tags/plasticity.tag
